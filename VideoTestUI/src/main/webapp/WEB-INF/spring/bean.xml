<?xml version="1.0" encoding="windows-1252"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id = "uploadDefaultPath" class="java.lang.String">
		<constructor-arg value="C:/zzz/upload"></constructor-arg>
	</bean>
	
	<bean id = "videoUploadDefaultPath" class="java.lang.String">
		<constructor-arg value="C:/zzz/video/upload"></constructor-arg>
	</bean>
	
	<bean id = "createZipPath" class="java.lang.String">
		<constructor-arg value="C:/zzz/download"></constructor-arg>
	</bean>
	
	<bean id = "tempFilesPath" class="java.lang.String">
		<constructor-arg value="C:/zzz/temp_files"></constructor-arg>
	</bean>
</beans>